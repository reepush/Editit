{
  "name": "editit",
  "version": "0.0.4",
  "description": "Edit your local files in browser",
  "main": "server.js",
  "scripts": {
    "build": "browserify -d client/script.js -o client/bundle.js",
    "default": "nodemon -i client/bundle.js -x 'npm run build && npm run start'"
  },
  "keywords": [
    "edit",
    "grep",
    "search",
    "local files",
    "fs"
  ],
