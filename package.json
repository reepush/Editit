    "angular-ui-codemirror": "^0.2.1",
    "angular-route": "^1.3.15",
    "browser-sync": "^2.3.1",
    "browserify": "^9.0.3",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.1.0",
    "gulp-jade": "^1.0.0",
    "gulp-plumber": "^1.0.0",
    "gulp-sass": "^1.3.3",
    "gulp-util": "^3.0.4",
    "highlight.js": "^8.4.0",
    "nodemon": "^1.3.7",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^2.4.0"
  },
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.9.3",
    "bootstrap": "^3.3.4",
